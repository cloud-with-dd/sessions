# Create cluster using eksctl
eksctl create cluster -f ./eksctl-cluster-config.yaml


# Build multi-arch container image
docker buildx build \
--push \
--platform "linux/arm/v7,linux/arm64/v8,linux/amd64" \
--tag "cloudwithdd/webapi:v0.4" .


